# Frontend Environment Variables
# Copy this file to .env and modify as needed

# API URL for the Express server (default: http://localhost:3001)
VITE_API_URL=http://localhost:3001

# Server Environment Variables
# Copy these to server/.env if you want to configure the server separately

# Port for the Express server (default: 3001)
PORT=3001

# External AI API URL (default: https://aiart-zroo.onrender.com/api/generate)
EXTERNAL_AI_API_URL=https://aiart-zroo.onrender.com/api/generate 